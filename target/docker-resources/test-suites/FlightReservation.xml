		<classes>
			<class name="com.pseudofunc.tests.flightreservation.FlightReservationTest"></class>
		</classes>
	</test>
	
	<test name="passengers-count-4">
		<parameter name="numOfPassengers" value="4"></parameter>
		<parameter name="totalPrice" value="$2338 USD"></parameter>
		<classes>
			<class name="com.pseudofunc.tests.flightreservation.FlightReservationTest"></class>
		</classes>
	</test>
</suite>