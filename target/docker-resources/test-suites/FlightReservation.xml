<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="flightReservation">
	<test name="passengers-count-1">
		<parameter name="numOfPassengers" value="1"></parameter>
		<parameter name="totalPrice" value="$584 USD"></parameter>
		<classes>
			<class name="com.pseudofunc.tests.flightreservation.FlightReservationTest"></class>
		</classes>
	</test>
	
	<test name="passengers-count-2">
		<parameter name="numOfPassengers" value="2"></parameter>
